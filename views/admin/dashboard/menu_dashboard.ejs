            <a href="/admin/data" style="text-decoration: none; color: grey;"
                >PASIEN</a
            >
        </h1>
    </div>
    <% laporan.map((data,i = 0) => { %>
    <a
        href="/admin/data/detail/<%= data.id %>"
        style="text-decoration: none; color: grey;"
    >
        <div class="row mb-3" data-aos="fade-up">
            <div class="col-md-12 justify-content-center align-items-center">
                <div
                    class="card border-left-<%= data.news <= 2 ? 'success' : data.news <= 5 ? 'warning' : data.news < 6 ? 'danger' : 'danger' %>"
                >
                    <div class="card-header">
                        <%= data.nama %>
                    </div>
                    <div class="card-body">
                        <div class="row d-flex align-items-center">
                            <div class="col-4 d-flex">
                                <div class="row">
                                    <canvas id="myPieChart<%=i + 1 %>"></canvas>
                                </div>
                            </div>
                            <div class="col-6">
                                <ul class="data">
                                    <li>Nama : <%= data.nama %></li>
                                    <li>Jenis kelamin : <%= data.jk %></li>
                                    <li>Asal Rumah Sakit : <%= data.asal %></li>
                                    <li>Usia : <%= data.usia %></li>
                                    <li>News/Pews : <%= data.news %></li>
                                    <li>
                                        Waktu Masuk: <%= data.timingMasuk %>
                                    </li>
                                </ul>
                            </div>
                            <div class="col-md-2">
                                <p>Waktu Respon</p>
                                <p class="time"><%= data.responTime %></p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </a>
    <% }) %>
    <input
        type="hidden"
        value="<%= data %>"
        name="laporanData"
        id="laporanData"
        class="laporanData"
    />
