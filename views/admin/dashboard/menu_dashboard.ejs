                        <h6 class="m-0 font-weight-bold text-primary">
                            Data Laporan
                        </h6>
                    </div>
                    <div class="card-body">
                        <div class="chartMenu">
                            <input
                                onchange="filterData()"
                                type="date"
                                value="2023-05-01"
                                id="startDate"
                            />
                            <input
                                onchange="filterData()"
                                type="date"
                                value="2023-05-03"
                                id="endDate"
                            />
                        </div>
                        <div class="chartCard">
                            <div class="chartBox">
                                <canvas id="myChart"></canvas>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
