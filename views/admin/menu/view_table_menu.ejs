<div class="card shadow mb-4">
    <div class="card-header py-3"></div>
    <div class="card-body">
        <div class="table-responsive">
            <table
                class="table table-bordered"
                id="tableBank"
                width="100%"
                cellspacing="0"
            >
                <thead>
                    <tr>
                        <th>Tanggal</th>
                        <th>Item</th>
                        <th>Jumlah</th>
                        <th>Lokasi</th>
                        <th>Admin</th>
                        <th>Keterangan</th>
                        <th>Action</th>
                    </tr>
                </thead>
                <tbody>
                    <% items.map((data,i) => { %>
                    <tr>
                        <td>
                            <%= data.createdAt.toLocaleString().split(',')[0] %>
                        </td>
                        <td><%= data.name %></td>
                        <td><%= data.qty %></td>
                        <td><%= data.lokasiId.name %></td>
                        <td>Galuh</td>
                        <td><%= data.keterangan %></td>
                        <td>
                            <a
                                href="javascript:void()"
                                type="button"
                                class="btn btn-warning btn-circle btn-sm button-edit"
                                data-id="<%= data.id %>"
                                data-name="<%= data.name %>"
                                data-namal="<%= data.lokasiId.name %>"
                                data-lokasi="<%= data.lokasiId.id %>"
                                data-qty="<%= data.qty %>"
                                ><i class="fas fa-edit"></i
                            ></a>
                            <form
                                action="/admin/barang/?_method=DELETE"
                                method="POST"
                                class="d-inline"
                            >
                                <button
                                    type="submit"
                                    class="btn btn-danger btn-circle btn-sm"
                                    onclick="return confirm('Anda yakin ingin dihapus')"
                                >
                                    <i class="fas fa-trash"></i>
                                </button>
                            </form>
                        </td>
                    </tr>
                    <% }) %>
                </tbody>
            </table>
        </div>
    </div>
</div>
