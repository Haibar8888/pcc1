<!-- alert -->
<% if (alert.message != '') { %>
<div
  class="alert alert-<%= alert.status %> alert-dismissible fade show mb-2"
  role="alert"
>
  <%= alert.message %>
  <button type="button" class="close" data-dismiss="alert" aria-label="Close">
    <span aria-hidden="true">&times;</span>
  </button>
</div>
<% } %>
<!-- end alert -->
